input "in.bib";

go format(
    enable last_comma,
    set indentation to (tab, 1),
    set string_identifier to quotes,
    set line_wrap to 80
);

go order(
    action filter ('essay91890', 'essay92294')
);

go content(
    action change_type ('misc', 'article')
);

field ('abstract', 'month') remove;

output "out.bib";